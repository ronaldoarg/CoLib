CREATE TABLE usuarios2 (
	id int auto_increment primary key,
	nome varchar(50),
	sobrenome varchar(50),
	email varchar(100)
);

CREATE TABLE livros2 (
	id int auto_increment primary key,
	nome varchar(50),
	autor varchar(50),
	edicao int,
	dono int,
	disponivel bool
);

CREATE TABLE mensagens2 (
	id int auto_increment primary key,
	de_id int,
	de_nome varchar(50),
	para int,
	assunto varchar(100),
	conteudo varchar(500),
	visivel bool
);
